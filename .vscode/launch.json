{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "chrome",
			"request": "launch",
			"sourceMapPathOverrides": {
				"webpack:///./js/*": "${workspaceRoot}/js/*",
				"webpack:///js/*": "${workspaceRoot}/js/*"
			},
			"sourceMaps": true,
			"name": "Launch Chrome against localhost",
			"url": "http://localhost:8080",
			"webRoot": "${workspaceRoot}/html"
		},
		{
			"type": "chrome",
			"request": "attach",
			"name": "Attach to Chrome",
			"port": 9222,
			"webRoot": "${workspaceRoot}/html"
		}
	]
}